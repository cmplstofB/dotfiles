Host github.com
	User git
	IdentityFile ~/.ssh/id.d/gh.ed_25519

Host cas*
	User cmplstofb
	HostName cmplstofb.ddns.net
	Port 10022
	IdentityFile ~/.ssh/id.d/cas.rsa_4096
	ForwardX11 yes
	Ciphers +aes256-ctr,aes256-gcm@openssh.com

Host cas_mc
	RequestTTY no
	#SessionType none
	LocalForward 25565 127.0.0.1:25565

Host cas_trpg
	RequestTTY no
	#SessionType none
	LocalForward 11080 127.0.0.1:11080
	LocalForward 11180 127.0.0.1:11180
